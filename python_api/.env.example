# Environment Variables Template
# Copy this file to .env and fill in your actual values

# Hugging Face Token (REQUIRED)
# Get your token from: https://huggingface.co/settings/tokens
# This is needed for PyAnnote speaker diarization and other HF models
HF_TOKEN=your_huggingface_token_here

# RunPod Configuration (for RunPod deployment)
RUNPOD_POD_ID=your_pod_id_here

# Optional: Override default model configurations
# WHISPER_MODEL_SIZE=medium
# OLLAMA_MODEL=hermes3:8b

# Optional: API Configuration
# API_HOST=0.0.0.0
# API_PORT=8000

# Performance Settings for RTX4090
TORCH_CUDA_ARCH_LIST=8.6
CUDA_VISIBLE_DEVICES=0
